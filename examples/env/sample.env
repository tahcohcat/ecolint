# examples/env/sample.env -
# A general representative example

# Database configuration
DATABASE_URL=postgresql://localhost:5432/myapp
DATABASE_POOL_SIZE=10

# API Configuration
API_BASE_URL=https://api.example.com
API_TIMEOUT=30

# Server configuration
PORT=3000
HOST=localhost
NODE_ENV=development

# Feature flags
ENABLE_LOGGING=true
ENABLE_METRICS=false

---

# examples/env/duplicates.env - Contains duplicate variables
DATABASE_URL=postgresql://localhost:5432/myapp
API_KEY=your_api_key_here
PORT=3000

# Oops, duplicate!
DATABASE_URL=postgresql://localhost:5432/different_db
API_KEY=another_key_here

# More config
DEBUG=true

---

# examples/env/security.env - Contains potential security issues
# Database
DATABASE_URL=postgresql://user:supersecret123@localhost:5432/prod

# API Keys - these look like real secrets!

# OAuth
GOOGLE_CLIENT_SECRET=GOCSPX-1234567890abcdefghijklmnopqrstuv
AWS_SECRET_ACCESS_KEY=wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY

# More secure variables
DATABASE_PASSWORD=my_super_secret_password_123
PRIVATE_KEY=-----BEGIN RSA PRIVATE KEY-----
MIIEpAIBAAKCAQEA7qwQlnKnN8...

---

# examples/env/convention.env - Naming convention issues
# Bad naming conventions
database-url=postgresql://localhost:5432/myapp
apiKey=your_api_key_here
Port=3000
some variable with spaces=value
cfg_tmp_dir=/tmp
usr_pwd=secret

# Redundant prefixes
ENV_DATABASE_URL=postgresql://localhost:5432/myapp
CONFIG_API_KEY=your_key_here
ENVIRONMENT_DEBUG=true

# Values that need attention
TRAILING_SPACE=value with trailing space
LEADING_SPACE=   value with leading space
UNQUOTED_SPACES=this value has spaces but no quotes
COMMENT_VALUE=#this looks like a comment

# Too short and too long names
X=short
EXTREMELY_LONG_VARIABLE_NAME_THAT_GOES_ON_AND_ON_AND_EXCEEDS_REASONABLE_LENGTH_LIMITS=value

---

# examples/env/malformed.env - Syntax errors
# Good variables
VALID_VAR=value
ANOTHER_VALID=another_value

# Malformed lines
MISSING_VALUE
=MISSING_KEY
MULTIPLE=EQUALS=SIGNS=HERE

# Empty key
=some_value

# Invalid characters in key
INVALID KEY=has spaces in key
INVALID-KEY=has hyphens
INVALID.KEY=has dots

# Empty values
EMPTY_VALUE=
ANOTHER_EMPTY=